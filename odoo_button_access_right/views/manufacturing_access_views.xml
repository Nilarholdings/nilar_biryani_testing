<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="mrp_production_form_view_button_access_inherit" model="ir.ui.view">
            <field name="name">mrp.production.form.inherit</field>
            <field name="model">mrp.production</field>
            <field name="priority">98</field>
            <field name="inherit_id" ref="mrp.mrp_production_form_view"/>
            <field name="arch" type="xml">
                <xpath expr="//button[@name='action_confirm']" position="attributes">
                    <attribute name="groups">odoo_button_access_right.group_confirm_access_mo</attribute>
                </xpath>
                <xpath expr="//button[@name='button_mark_done'][1]" position="attributes">
                    <attribute name="groups">odoo_button_access_right.group_mark_as_done_access_mo</attribute>
                </xpath>
                <xpath expr="//button[@name='button_mark_done'][2]" position="attributes">
                    <attribute name="groups">odoo_button_access_right.group_mark_as_done_access_mo</attribute>
                </xpath>
                <xpath expr="//button[@name='button_mark_done'][3]" position="attributes">
                    <attribute name="groups">odoo_button_access_right.group_mark_as_done_access_mo</attribute>
                </xpath>
                <xpath expr="//button[@name='button_mark_done'][4]" position="attributes">
                    <attribute name="groups">odoo_button_access_right.group_mark_as_done_access_mo</attribute>
                </xpath>
                <xpath expr="//button[@name='do_unreserve']" position="attributes">
                    <attribute name="groups">odoo_button_access_right.group_unreserve_access_mo</attribute>
                </xpath>
                <xpath expr="//button[@name='action_toggle_is_locked'][1]" position="attributes">
                    <attribute name="groups">odoo_button_access_right.group_lock_unlock_access_mo</attribute>
                </xpath>
                <xpath expr="//button[@name='action_toggle_is_locked'][2]" position="attributes">
                    <attribute name="groups">odoo_button_access_right.group_lock_unlock_access_mo</attribute>
                </xpath>
                <xpath expr="//button[@name='button_unbuild']" position="attributes">
                    <attribute name="groups">odoo_button_access_right.group_unbuild_access_mo</attribute>
                </xpath>
                <xpath expr="//button[@name='action_cancel'][1]" position="attributes">
                    <attribute name="groups">odoo_button_access_right.group_cancel_access_mo</attribute>
                </xpath>
                <xpath expr="//button[@name='action_cancel'][2]" position="attributes">
                    <attribute name="groups">odoo_button_access_right.group_cancel_access_mo</attribute>
                </xpath>
            </field>
        </record>
        <record id="mrp_production_form_view_inherit_button_access_inherit" model="ir.ui.view">
            <field name="name">mrp.production.form.inherit</field>
            <field name="model">mrp.production</field>
            <field name="priority">99</field>
            <field name="inherit_id" ref="mrp_to_produce.mrp_production_form_view_inherit"/>
            <field name="arch" type="xml">
                <xpath expr="//button[@name='button_mo_produce']" position="attributes">
                    <attribute name="groups">odoo_button_access_right.group_create_mo_produce_access_mo</attribute>
                </xpath>
            </field>
        </record>
    </data>
</odoo>